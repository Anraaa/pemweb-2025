                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Example Product"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Optional description"
